<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Oracle Corporation//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
    <servlet>
        <servlet-name>submitForm</servlet-name>
        <servlet-class>myapp.SubmitFormServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>submitForm</servlet-name>
        <url-pattern>/submit_form</url-pattern>
    </servlet-mapping>
	<servlet>
        <servlet-name>viewArea</servlet-name>
        <servlet-class>myapp.ViewAreaServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>viewArea</servlet-name>
        <url-pattern>/view_area</url-pattern>
    </servlet-mapping>
	<servlet>
        <servlet-name>MLS</servlet-name>
        <servlet-class>myapp.MLSServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MLS</servlet-name>
        <url-pattern>/view_mls</url-pattern>
    </servlet-mapping>
	<servlet>
        <servlet-name>ViewAllCustomers</servlet-name>
        <servlet-class>myapp.ViewAllCustomersServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewAllCustomers</servlet-name>
        <url-pattern>/view_customers</url-pattern>
    </servlet-mapping>
	<servlet>
        <servlet-name>Enqueue</servlet-name>
        <servlet-class>myapp.Enqueue</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Enqueue</servlet-name>
        <url-pattern>/enqueue</url-pattern>
    </servlet-mapping>
	<servlet>
        <servlet-name>Worker</servlet-name>
        <servlet-class>myapp.Worker</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Worker</servlet-name>
        <url-pattern>/worker</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>Admin_home.html</welcome-file>
    </welcome-file-list>
</web-app>
